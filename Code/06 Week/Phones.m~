clear;

% Customers are represented by the vector

C = [500 500]';

% an n x n matric is called a MArkov MAtrix if all entries are non-negative
% and each column adds up to 1


A = [2/3 1/2; 1/3 1/2];

N = 10;
out = zeros(N,3);
out(:,1) = 1:N;



for i = 1:N
    C = A * C;
    m(i,2

end



