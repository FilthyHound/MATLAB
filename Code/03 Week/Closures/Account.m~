function [fh_setBalance, getBalance, debit, credit] = Account(num, bal)
% This is an example of an embedded function
number = num;
bal = 0.0;


function  [] =  setBalance(newb)
 bal= newb;
end

function [the_bal] =  getBalance()
the_bal = bal;
end

getBalance = @getBalance;
setBalance = @setBalance;


end

